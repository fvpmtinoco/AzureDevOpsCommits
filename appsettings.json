{
  "DevOpsCredentials": {
    "PAT": "YOUR_PAT",
    "BaseURL": ""
  },
  "FileExtensions": {
    "AdmissibleExtensions": [
      ".cs",
      ".yml",
      ".html",
      ".py",
      ".scss",
      ".js",
      ".ts"
    ]
  },
  "Repositories": {
    "Names": [
      ""
    ],
